<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.heckaitor.demo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".bezier.BezierActivity"/>
        <activity android:name=".interceptradio.InterceptRadioActivity"/>
        <activity android:name=".image.ImageScaleActivity"/>
        <activity
            android:name=".asyncdialog.AsyncTaskActivity"
            android:configChanges="orientation|screenSize"/>
        <activity android:name=".permission.PermissionActivity"/>
        <activity android:name=".popw.PopupWindowActivity"/>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings">
        </activity>
        <activity
            android:name=".touch.TouchDelegateActivity"
            android:label="@string/title_activity_touch_delegate">
        </activity>
        <activity android:name=".recycler.RecyclerViewActivity">
        </activity>
        <activity android:name=".coord.ViewCoordinateActivity">
        </activity>
        <activity android:name=".list.ListViewActivity">
        </activity>
        <activity android:name=".anim.AlphaControlActivity">
        </activity>
    </application>

</manifest>